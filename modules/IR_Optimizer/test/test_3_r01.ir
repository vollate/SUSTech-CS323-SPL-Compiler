FUNCTION mod :
PARAM v0
PARAM v1
t0 := v0 / v1
t1 := t0 * v1
t2 := v0 - t1
RETURN t2

FUNCTION isPalindrome :
PARAM v2
v3 := #0
v4 := v2
v5 := #0
LABEL label0 :
IF v2 != #0 GOTO label1
GOTO label2
LABEL label1 :
ARG #10
ARG v2
v5 := CALL mod
t4 := v3 * #10
v3 := t4 + v5
v2 := v2 / #10
GOTO label0
LABEL label2 :
IF v3 == v4 GOTO label3
GOTO label4
LABEL label3 :
RETURN #1
LABEL label4 :
RETURN #0

FUNCTION main :
READ v6
ARG v6
t8 := CALL isPalindrome
IF t8 == #1 GOTO label6
GOTO label7
LABEL label6 :
WRITE #1
GOTO label8
LABEL label7 :
t9 := #0 - #1
WRITE t9
LABEL label8 :
RETURN #0
